  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/geojson": "^7946.0.10",
    "microbundle": "^0.15.1",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.2",
    "typescript": "^5.0.4"
  }
